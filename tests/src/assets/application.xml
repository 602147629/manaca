<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="www.manaca.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
    <appSettings>
        <version>1.0</version>
        <securitySettings>
            <crossDomainPolicies>
                <url/>
            </crossDomainPolicies>
            <allowedDomains>
                <domain/>
            </allowedDomains>
        </securitySettings>
        <!-- set debug level to debug|info|warn|error|fatal|off -->
        <loggingSettings enabled="true" logLevel="debug">
            <Output outputHeight="150" logLevel="debug" strong="true"/>
            <TracePublisher logLevel="debug"/>
        </loggingSettings>
    </appSettings>
    <extendFile>assets/settings.xml</extendFile>
    <projectSettings>
        <add key="TEST_VALUE1" value="value1"/>
        <add key="TEST_VALUE2" value=""/>
    </projectSettings>
    <files>
        <file url="assets/settings.xml" name="file" fileType="xml"/>
    </files>
</configuration>
